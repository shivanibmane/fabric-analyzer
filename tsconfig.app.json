{"extends": "./tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx", // or "react" if using React <17
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
